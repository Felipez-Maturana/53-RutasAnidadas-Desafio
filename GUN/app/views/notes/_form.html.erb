<%= form_for([@user.group,@user,@note]) do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
   
  <div class="field">
    <%= f.label :user %><br />
    <%= f.hidden_field :user %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>